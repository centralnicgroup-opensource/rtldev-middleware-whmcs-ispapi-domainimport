    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/hexonet/whmcs-ispapi-helper.git"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "hexonet/whmcs-ispapi-helper": "1.0.5"
    },
    "require-dev": {
        "phpunit/phpunit": "@stable",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "scripts": {
        "codeCheck": "phpcs --standard=PSR2 -q -n --colors --ignore=*/vendor/* modules tests",
        "codeFix": "phpcbf --standard=PSR2 -q --ignore=*/vendor/* modules tests",
        "test": "phpunit -c phpunit.xml"
    },
    "config": {
        "vendor-dir": "./modules/addons/ispapidomainimport/lib/vendor"
    }
